{
	"name": "woa",
	"version": "0.0.64",
	"description": "Server for World of Atoms",
	"scripts": {
		"start": "cross-env NODE_ENV=development nodemon -L ./engine --inspect --exec babel-node --presets env,stage-2",
		"serve": "cross-env NODE_ENV=production node distribution/index.js",
		"postnstall": "babel-node scripts/preinstall.js --presets env,stage-2",
		"build:engine": "cross-env NODE_ENV=production babel engine --out-dir distribution --source-maps cheap-module-source-map",
		"build:libs": "babel libs --out-dir distribution/libs --copy-files",
		"build": "npm run build:engine && npm run build:libs"
	},
	"engines": {
		"node": ">=8.9.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Weemee/woa.git"
	},
	"author": "Chris Jager",
	"license": "CC-BY-3.0",
	"bugs": {
		"url": "https://github.com/Weemee/woa/issues"
	},
	"nodemonConfig": {
		"ignore": [
			"node_modules/*",
			"config.json"
		]
	},
	"dependencies": {
		"babel-preset-stage-2": "^6.24.1",
		"babel-runtime": "^6.26.0",
		"bcrypt": "^1.0.3",
		"bluebird": "^3.5.1",
		"body-parser": "^1.18.2",
		"chalk": "^2.3.2",
		"content-filter": "^1.1.2",
		"dotenv": "^5.0.1",
		"express": "^4.16.2",
		"helmet": "^3.12.0",
		"jsonwebtoken": "^8.2.0",
		"mkdirp": "^0.5.1",
		"mysql2": "^1.5.3",
		"passport": "^0.4.0",
		"passport-local": "^1.0.0",
		"passport-totp": "0.0.2",
		"sequelize": "^4.37.3",
		"socket.io": "^2.0.4",
		"thirty-two": "^1.0.2",
		"uuid": "^3.2.1"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-eslint": "^8.2.2",
		"babel-plugin-module-resolver": "^3.1.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-register": "^6.26.0",
		"cross-env": "^5.1.4",
		"eslint": "^4.19.1",
		"nodemon": "^1.17.1"
	},
	"homepage": "https://github.com/Weemee/woa#readme"
}
